  "dependencies": {
    "compression": "^1.8.1",
    "sirv": "^3.0.2",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "cross-env": "^10.1.0",
    "express": "^5.2.1",
    "vite": "^7.2.4"
  }
}